  <script type="text/javascript">
    $(document).ready(function() {
      $("img").unveil(150);
      $("#false").hide();
      nilai_star();
    });
  
  /* fungsi untuk menampilkan captcha */
  function captcha_views() {
   
      var url="https://mizanstore.com/cart_catalog/test_adi/f_LoadCapcha";
      
      $.ajax({
        url: url,
        type: 'GET',
        dataType: 'json',
        success: function(data){
          //console.log(data.recap);
          if(data.word == ''){
            $('#recap-goggle').hide();
          }
        }
      });
    };
  
  
    var pencarian_modal = function(){
      $('#pencarian-modal').modal('show');
    }
    var modal_detail_buku = function(produk_id){
      var url="https://mizanstore.com/cart_catalog/produk/detail_json";
      // alert(url);
  
  
  
      $.ajax({
        url: url+'/'+produk_id,
        type: 'GET',
        dataType: 'json',
        success: function(json){
          if(json.status == true){
            $('#qv-beli').show();
  
            // console.log(json);
            var url_gambar = "https://inc.mizanstore.com/aassets/img/com_cart/produk/"+json.detail.gambar_thumb;
            $('#detail_img').attr('src', url_gambar);
            $('#detail_img').attr('title', json.detail.nama);
            $('#detail_status_produk').html(json.detail.nama_status);
            var html ="";
            for(i=0; i < json.nilai_star; i++){
              html +='<li><i class="fa fa-star"></i></li>';
            }
              if(json.total_vote > 0 ){
                html +='<li>'+json.total_vote+' Ulasan</li>';
              }else{
                html +='<li>Belum ada Ulasan</li>';
              }
              $('#detail_bintang').html(html);
              html ="";
              html +=json.detail.keterangan;
              html +=' <a href="'+json.detail.link_produk+'" style="color:blue;">Baca Selengkapnya</a>'
              $('#detail_judul_produk').html(json.detail.nama);
              $('#detail_diskon_produk').html('Rp '+json.detail.harga_lama);
              $('#detail_harga_produk').html('Rp '+json.detail.harga);
              $('#qv-wishlist').attr('href',"https://mizanstore.com/cart_order/cart/add_wishlist/"+produk_id);
              $("#detail_keterangan_produk").html(html);
              $('#detail_cart_id').val(produk_id);
              $('#detail_cart_qty').val(1);
              $('#qv-beli').attr('id','addtocart_'+produk_id);
              $('#quick-view').modal('show');
              if (json.detail.tombol == undefined){
                 $('#qv-beli').hide();
              }
            /*
             */
          }
        }
      });
    }
  
    $('#detail_cart').submit(function(event){
      event.preventDefault();
      var detail_cart = $('#detail_cart').serializeArray();
      // console.log(detail_cart[0].value);
      var url = "https://mizanstore.com/cart_order/cart_api/add_test/"+detail_cart[0].value;
      if(detail_cart[1].value  < 1){
        detail_cart[1].value = 1;
      }
      $.post(url, { qty: detail_cart[1].value }, function(data, textStatus, xhr) {
          
          // $('#pesan_notif').html(data.pesan);
          
  
            var url="https://mizanstore.com/cart_order/cart_api/ringkasan";
  
              $.getJSON(url, { }, function(json, textStatus) {
                /*optional stuff to do after success */
                $.each(json,function(index, el) {
                  if(el.nama == "Total Item"){
                    $('#total_items_cart').html(el.value)
                  }
                });
  
              });
  
          if(data.status && data.kode == '1'){
             $('#quick-view').modal('hide');
             window.location.href = "https://mizanstore.com/keranjang";
          }else{
            //console.log(data);
            if(!data.status && data.kode == 2 && data.url_redirek !=undefined){
              window.location.href = data.url_redirek;
            }else {
              if(data.status==false && data.kode==2){
                $('#ms_content').addClass('alert-danger');
                $('#msg_content').slideDown();
                
                $('#msg_content').html(data.pesan);
                Kuartesimeout(function(){ $('#msg_content').slideUp(); }, 3000);
              }
            }
          }
      });
    })
  
  </script>
  
