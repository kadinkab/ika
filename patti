    var gen_captcha = function () {
      var url_captcha = "https://mizanstore.com/cart_catalog/reviewproduk/json_captcha";
      $.getJSON(url_captcha, {},
        function (data, textStatus, jqXHR) {
          $("#captcha").html(data.image);
        }
      );
    }
  
    var nilai_star = function (){
      var id_produk = $("input[name='produk_id']").val();
      var url = "https://mizanstore.com/cart_catalog/test_adi/f_GetNilaiStar/"+id_produk;
      
      var html ='';
      $.getJSON(url, {}, function (json, textStatus) {
            if (json.total_vote > 0) {
              for (count = 1; count <= json.nilai_star; count++) {
                html += '<i class="fa fa-star" style="color:#F0BF2D;"></i>';
              }
              html += '<li style="margin-right:5px;">';
                html += '<a href="#tab-1" onclick="sembunyikan("#tab-1")">'+json.total_vote+' customer review</a>';
                html += '</li>';
            }
            //console.log(html);
            $('#nilai_star').html(html);
      });
  
    }
  
    var reviews_list = function (){
      var id_produk = $("input[name='produk_id']").val();
      var url = "https://mizanstore.com/cart_catalog/produk_detail/list_reviews/"+id_produk;
      //console.log(url);
      var html ='';
      $.getJSON(url, {}, function (json, textStatus) {
          
            if (json.jumlah > 0) {
              html += '<ul id="reviews-list">';
              $.each(json.list_reviews, function(i, review) {
                //console.log(review);
                    html += '<li id="review_ids_' +review.review_id+' ">';
                    html += '<img src="https://inc.mizanstore.com/assets/img/sample/avatar/avatar.png" ALT="avatar">';
                    html += '<div class="comment">';
                    html += '<div class="reviews-detail">';
                    if(review.customer == null){
                        var customer = 'Customer';
                    }else{
                      var customer = review.customer;
                    }
                    html += '<h6>'+customer+ '<span>'+review.date_added+'</span></h6>';
                    html += '<ul class="rating-stars">';
                      for (start=1; start <= review.star_produk; start++){
                        html += '<li><i class="fa fa-star"></i></li>';
                      }
                      
                    html += '</ul>';
                    html += '</div>';
                    html += '<p>'+review.review+'</p>';
                    html += '<a href="#review_ids_'+review.review_id+'" id="tombol_'+review.review_id+'" class="btn btn-xs btn-warning" data-id="'+review.review_id+'"';
                    html += 'data-count="0" onclick="tampilkan_balasan('+review.review_id+')">balas review</a>';
                    html += '</div>';
                    html += '<ul style="margin-top:5%;" id="daftar_balasan_'+review.review_id+'">';
                    if (review.subs > 0){
                      $.each(review.subs, function(i, i2) {
                        html += '<li style="margin-left:10%;">';
                        html += '<img src="https://inc.mizanstore.com/assets/img/sample/avatar/avatar.png" ALT="avatar">';
                        html += '<div class="comment"><div class="reviews-detail">';
                            if (i2.customer_id == 1188996){
                              html += '<h6 style="color:aqua;"> Administrator <span>'+i2.date_added+'</span></h6>';
                            }
                            if (i2.customer_id != 1188996){
                              html += '<h6>'+i2.customer+'<span>'+i2.date_added+'</span></h6>';
                            }
  
                        html += '</div>';
                        html += '<p>'+i2.review+'</p></div></li>';
                      });
                    }
                    html += '</ul></li>';
                  });
                html += '</ul>';
                }
                if (json.jumlah == 0){
                html += '<ul><li><strong>Belum Ada Review</strong></li></ul>';
                }
            $('#reviews_list').html(html);
      });
  
    }
  
    var simpan_review = function () {
      /* 1975 - 2003*/
      
      var star_produk = $("input[name='star_produk']").val();
      var produk_id = $("input[name='produk_id']").val();
      var review = $("textarea[name='review']").val();
      var captcha = $("input[name='captcha']").val();
      var g_recaptcha_response = $("#g-recaptcha-response").val();
      var url_simpan = "https://mizanstore.com/cart_catalog/reviewproduk/simpan_review_ajax";
       // console.log(g_recaptcha_response);
      var html = "";
  
      $.post(url_simpan, {
          star_produk: star_produk,
          produk_id: produk_id,
          review: review,
          g_recaptcha_response:g_recaptcha_response
        },
        function (data, textStatus, jqXHR) {
          // gen_captcha();
          
          // $("input[name='captcha']").val('');
          $("#review_text").val('');
          $('#notif-review').html(data.pesan);
          if (data.status && data.kode == 1) {
  
            html += '<li id="review_ids_' + data.ids + '">';
            html += '<img src="http://inc.mizanstore.com/assets/img/sample/avatar/avatar.png" ALT="avatar">';
            html += '<div class="comment">';
            html += '<div class="reviews-detail">';
            html += '<h6>';
            html += '<span>' + data.posting.date_added + '</span>';
            html += '</h6>';
            html += '<ul class="rating-stars">';
            if (star_produk > 0) {
              for (index = 1; index <= star_produk; index++) {
                html += '<li>';
                html += '<i class="fa fa-star"></i>';
                html += '</li>';
  
              }
            }
            html += '</ul>';
            html += '</div>';
            html += '<p>' + review + '</p>';
            html += '<a href="#review_ids_' + data.ids + '" id="tombol_' + data.ids +
              '" class="btn btn-xs btn-warning" data-id="' + data.ids +
              '" data-count="0" onclick="tampilkan_balasan(' + data.ids + ')">balas review</a>';
            html += '</div>';
            html += '<ul style="margin-top:5%;" id="daftar_balasan_' + data.ids + '">';
            html += '</ul>';
            html += '</li>';
  
            $('#reviews-list').append(html);
  
          } else {
  
          }
          grecaptcha.reset();
          // console.log(data);
        },
        "json"
      );
      // alert(star_produk);
    }
  
  
    var url_goodread = $('.gr_branding').attr('href');
    $('.gr_branding').attr('href', "https://mizanstore.com/landing/goodreads?url_goodreads=" + url_goodread +
      "&url_back=as_long_as_the_72226/");
    var sembunyikan = function (tab) {
      $('#tab-1').hide();
      $('#tab-2').hide();
      $('#tab-3').hide();
      $(tab).show();
      /*id="tb-2"
      id="tb-3"
      id="tb-1"*/
      if (tab == "#tab-2") {
        $('#tb-1').removeClass('tab-aktif');
        $('#tb-3').removeClass('tab-aktif');
        $('#tb-2').addClass('tab-aktif');
      }
      if (tab == "#tab-1") {
        $('#tb-2').removeClass('tab-aktif');
        $('#tb-3').removeClass('tab-aktif');
        $('#tb-1').addClass('tab-aktif');
        reviews_list();
      }
      if (tab == "#tab-3") {
        $('#tb-1').removeClass('tab-aktif');
        $('#tb-2').removeClass('tab-aktif');
        $('#tb-3').addClass('tab-aktif');
      }
    }
    sembunyikan('#tab-2');
  
    var baca_selengkapnya = function (tab) {
      $(tab).show();
  
      if (tab == "#tab-2") {
        $('#tb-1').removeClass('tab-aktif');
        $('#tb-3').removeClass('tab-aktif');
        $('#tb-2').addClass('tab-aktif');
      }
    }
    baca_selengkapnya('#tab-2');
  
   
  
  
    var buku_by_kategori = function () {
      // http://modular.develop.com/api/kategori_produk/list_produk/269/10/0?idkey=S9Rp 3YD7JL&passkey=XJX3LFSG3CF4Q91WB
      var kategori = "562";
      var url = "https://mizanstore.com/cart_catalog/Ctrl_api_produk/list_produk_kat/" + kategori;
      var gambar = "http://t3d.mizanstore.com/themes/book-store/images/best-seller/img-01.jpg";
      var html = '';
      $.getJSON(url, {}, function (json, textStatus) {
  
        // console.log(json);
        if (json.data.kode == 1) {
          $.each(json.data.produk_data, function (index, el) {
            html += '<li style="max-width: 122px;margin-right:12px;">';
            html += '<a href="' + el.link_produk + '" title="' + el.judul_produk_limit + '">';
            html += '<div class="cover">';
            html += '<img src="https://inc.mizanstore.com/aassets/img/com_cart/produk/' + el.gambar_thumb + '" ALT="'+ el.judul_produk_limit +'" style="max-height: 210px; width: 122px;">';
            html += '</div>';
            html += '<div style="min-height: 110px; max-height: 110px;">';
            html += '<span class="line-spasi">' + el.judul_produk_limit +'</span>';
            html += '<span class="penulis">'+el.nama_pengarang + '</span>';
            if (el.harga_lama == 0) {
              html += '<strong>Rp ' + el.harga_format + '</strong>';
              html += '<span class="harga-bruto"> <p>Rp' + el.harga_format + '</p></span>';
            }
            if (el.harga_lama > 0) {
            html += '<span class="harga-bruto"> <p class="coret-harga">Rp ' + el.harga_lama_format + '</p></span>';
            html += '<span class="font-harga">Rp ' + el.harga_format + '</span>';
            }
            html += '</div>';
            html += '<div class="ranting">';
            html += '<span class="os-rating">';
            if (el.total_vote > 0) {
              for (count = 1; count <= el.nilai_star; count++) {
                html += '<i class="fa fa-star" style="color:#ffc107!important"></i>';
              }
            }
            if (el.total_vote == 0) {
              html += '<span class="jumlah_vote"></span>';
  
            }
            if (el.total_vote > 0) {
              html += '<span class="jumlah_vote"> (' + el.total_vote + ') Ulasan</span>';
            }
            html += '</span>';
            html += '</div>';
            html += '</a>';
            html += '</li>';
  
          });
          $('#item_pbk').html(html);
        }
      });
    }
  
