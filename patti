  <script>
    $('#kata_pencarian_xx').autocomplete({
      source:"https://mizanstore.com/pencarian/autocomplete_cari",
      delay: 1000,
      select: function (a, b) {
        $(this).val(b.item.value);
        $("#form_pencarian_xx").submit()
      }
    }).autocomplete( "instance" )._renderItem = function( ul, item ) {
      //console.log(item);
      if(item.idx == "p_populer"){
        return $( "<li disable>" )
        .addClass("block-item")
        .append( "<div>" +item.label+ "</div>" )
        .appendTo( ul );
      }else{
        return $( "<li>" )
        // .addClass("block-item")
        .append( "<div style='margin:0px 10px 0px 10px;'>" + item.label + "<br>" + item.idx + "</div>" )
        .appendTo( ul );
      }
        
      };
  
    $('#kata_pencarian_xx2').autocomplete({
      source:"https://mizanstore.com/pencarian/autocomplete_cari",
      delay: 1000,
      select: function (a, b) {
        $(this).val(b.item.value);
        $("#form_pencarian_xx2").submit()
      }
    }).autocomplete( "instance" )._renderItem = function( ul, item ) {
      //console.log(item);
      if(item.idx == "p_populer"){
        return $( "<li disable>" )
        .addClass("block-item")
        .append( "<div>" +item.label+ "</div>" )
        .appendTo( ul );
      }else{
        return $( "<li>" )
        // .addClass("block-item")
        .append( "<div style='margin:0px 10px 0px 10px;'>" + item.label + "<br>" + item.idx + "</div>" )
        .appendTo( ul );
      }
        
      };
  
      
    
    function kirim_email() {
              var email = $("#email").val();
              //console.log(email);
              var dataString = '&email=' + email;
          
              $.ajax({
              type: "POST",
              url: "https://mizanstore.com/customer/newsletter/f_news_proses",
              data: dataString,
              success: function(data) {
                $('#pesan_news').html("<div id='message'></div>");
              }
              });
              //return false;
            };
  
    //AD20190220 cek email valid or invalid
    function ValEmail() {
          var email = document.getElementById("email").value;
          var urls = 'https://mizanstore.com/customer/newsletter/f_get_email';
        $.post(urls, { email: email }, function(data, textStatus, xhr) {
          obj = jQuery.parseJSON(data);
          dts = JSON.parse(obj);
                  if (dts.result == 'valid') {
                      $("#false").hide();
                      f_Disabled_Tombol(1);
                  }else{
                      f_Disabled_Tombol(0);
                      $("#false").show();
                      return;
                  }
                    
              });          
      }
  
      function f_Disabled_Tombol(t){
      if(t==0){
        $('#subscribe').removeClass('disabled');
        $('#subscribe').addClass('disabled');
        $('#subscribe').html('Loading...');
      }else{
        Kuartesimeout(function (){
          $('#subscribe').removeClass('disabled');
          $('#subscribe').html('Submit');
        }, 2500);			
      }
      return;
    }
  
    //AD20190312 Loading image lazy
    document.addEventListener("DOMContentLoaded", function() {
    var lazyloadImages = document.querySelectorAll("img.lazy");    
    var lazyloadThrottleTimeout;
    
    function lazyload () {
      if(lazyloadThrottleTimeout) {
      clearTimeout(lazyloadThrottleTimeout);
      }    
      
      lazyloadThrottleTimeout = Kuartesimeout(function() {
        var scrollTop = window.pageYOffset;
        lazyloadImages.forEach(function(img) {
          if(img.offKuartesop < (window.innerHeight + scrollTop)) {
          img.src = img.dataset.src;
          img.classList.remove('lazy');
          }
        });
        if(lazyloadImages.length == 0) { 
        document.removeEventListener("scroll", lazyload);
        window.removeEventListener("resize", lazyload);
        window.removeEventListener("orientationChange", lazyload);
        }
      }, 20);
    }
    
    document.addEventListener("scroll", lazyload);
    window.addEventListener("resize", lazyload);
    window.addEventListener("orientationChange", lazyload);
    });
  </script>
  
  
