    var tampilkan_balasan = function (id) {
  
       /* 2200 - 2213*/
      
      var count_detektor = $("#tombol_" + id).attr('data-count');
      if (count_detektor == 0) {
        $("#tombol_" + id).attr('data-count', 1);
        var html = "";
        html += '<li style="margin-left:10%;" id=rep_komentar_' + id + '>';
        html += '<div class="comment">';
        html += '<div class="reviews-detail">';
        html += '<div class="form-group">';
        html += '<label for="">Balasan Review</label>';
        html += '<textarea name="" id="komentar_' + id + '" cols="30" rows="4"></textarea>';
        html += '</div>';
        html += '<div class="form-group">';
        html += '<a href="#review_ids_' + id + '" onclick=posting_balasan(' + id +
          ') class="btn btn-primary btn-xs">Balas</a>';
        html += '</div>';
        html += '</div>';
        html += '</li>';
  
        $("#review_ids_" + id).append(html);
      }
  
    }
  
    var posting_balasan = function (id) {
      var review = $("#komentar_" + id).val();
      var url = "https://mizanstore.com/cart_catalog/reviewproduk/simpan_balasan_review"
      var html = "";
      $.post(url, {
          id_parrent: id,
          review: review
        },
        function (data) {
          //console.log(data);
          var response = JSON.stringify(data);
          //console.log(response);
          if (data.status) {
            html += '<div class="alert alert-success alert-dismissable">';
            html += '<button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>';
            html += data.pesan;
            html += '</div>';
          } else {
            html += '<div class="alert alert-danger alert-dismissable">';
            html += '<button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>';
            html += data.pesan;
            html += '</div>';
          }
  
          $.getJSON("https://mizanstore.com/cart_catalog/produk_url/review_id/" + id, data,
            function (data, textStatus, jqXHR) {
              var html_r = '';
              // console.log(data);
  
              $.each(data[0].subs, function (i, l) {
  
                // console.log(l.review);
  
                html_r += '<li style="margin-left:10%;">';
                html_r += '<img src="https://inc.mizanstore.com/assets/img/sample/avatar/avatar.png" ALT="">';
                html_r += '<div class="comment">';
                html_r += '<div class="reviews-detail">';
                if (l.customer_id == 1188996) {
                  html_r += '<h6 style="color:aqua;">Administrator <span>' + l.date_added + '</span></h6>';
                }
                if (l.customer_id != 1188996) {
                  if (l.customer == null) {
                    html_r += '<h6> <span>' + l.date_added + '</span></h6>';
  
                  } else {
                    html_r += '<h6>' + l.customer + ' <span>' + l.date_added + '</span></h6>';
  
                  }
                }
                html_r += '</div>';
                html_r += '<p>' + l.review + '</p>';
                html_r += '</div>';
                html_r += '</li>';
              });
  
              $("#daftar_balasan_" + id).html(html_r);
              $('#rep_komentar_' + id).remove();
              $("#tombol_" + id).attr('data-count', 0);
            }
          );
  
          $("#alert-x").html(html);
  
        }, 'json'
      );
    }
  </script>
  
  
  
  
