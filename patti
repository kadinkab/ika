    }(document, 'script', 'facebook-jssdk'));
  </script>
  
  <script type="text/javascript">
  
    $(function () {
  
      $("#rateYo").rateYo({
        rating: 0,
        precision: 0,
        spacing: "5px",
        maxValue: 5,
        multiColor: {
          "startColor": "#FF0000", //RED
          "endColor": "#FF8401" //GREEN
        },
        onSet: function (rating, rateYoInstance) {
          $('#rate').val(rating);
          // alert("Rating is set to: " + rating);
        }
      });
    });
    var ganti = function (url) {
